{
  "name": "markmap-cli",
  "version": "0.4.5",
  "description": "Create markmaps from CLI",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "bin": {
    "markmap": "bin/cli.js"
  },
  "keywords": [
    "markmap",
    "markdown",
    "mindmap"
  ],
  "homepage": "https://github.com/gera2ld/markmap/packages/markmap-cli#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gera2ld/markmap.git"
  },
  "scripts": {
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del-cli dist",
    "prebuild": "run-s ci clean",
    "prepublishOnly": "run-s build",
    "ci": "run-s lint",
    "lint": "eslint --ext .ts .",
    "build:types": "tsc",
    "build:js": "rollup -c rollup.conf.js",
    "build": "run-s build:types build:js"
  },
  "bugs": {
    "url": "https://github.com/gera2ld/markmap/issues"
  },
  "main": "dist/index.js",
  "files": [
    "bin",
    "dist",
    "types"
  ],
  "typings": "types/index.d.ts",
  "devDependencies": {
    "@babel/runtime": "^7.11.2",
    "markmap-lib": "^0.11.2"
  },
  "dependencies": {
    "chokidar": "^3.4.2",
    "commander": "^5.1.0",
    "koa": "^2.13.0",
    "open": "^7.0.3",
    "update-notifier": "^4.1.1"
  },
  "engines": {
    "node": ">=10"
  }
